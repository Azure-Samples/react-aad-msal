{
  "name": "react-aad-msal",
  "version": "2.2.2",
  "description": "A react component that integrates with Azure AD (v2, MSAL).",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/syncweek-react-aad/react-aad.git"
  },
  "main": "./dist/umd/react-aad-msal.js",
  "module": "./dist/commonjs/index.js",
  "es2015": "./dist/esm/index.js",
  "files": [
    "dist"
  ],
  "author": "Laura Bochenek",
  "contributors": [
    "Laura Bochenek <laura.bochenek@microsoft.com>",
    "Omeed Musavi <omusavi@microsoft.com>",
    "Lilian Kasem <lilian.kasem@microsoft.com>",
    "Tess DiStefano <tedistef@microsoft.com>",
    "Lucas Huet-Hudson <lucashh@microsoft.com>",
    "Zach Miller <zamiller@microsoft.com>",
    "P.J. Little <pj.little@microsoft.com>",
    "Shawn Cicoria <shawn.cicoria@microsoft.com>",
    "Andrew Craswell <andcra@microsoft.com>",
    "Bj√∂rn Dalfors <bjorn@binovi.se>"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint --config tslint.json --project ./tsconfig.json --fix",
    "lint:types": "tsc --project ./tsconfig.json --noEmit --pretty",
    "start": "npm run clean && npm run build:commonjs && cd ./sample && npm run start",
    "build": "npm run clean && npm run lint && npm run build:commonjs && npm run build:esm && npm run build:umd",
    "build:commonjs": "tsc",
    "build:esm": "tsc -m es6 --outDir dist/esm -t es6",
    "build:umd": "webpack",
    "build:watch": "npm run build:commonjs -- --watch",
    "test": "jest --env=jsdom",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "dependencies": {
    "redux": "4.0.4"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@types/jest": "24.0.18",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "auto-changelog": "1.16.2",
    "babel-jest": "24.9.0",
    "clean-webpack-plugin": "3.0.0",
    "core-js": "3.2.1",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "fork-ts-checker-webpack-plugin": "0.5.1",
    "husky": "3.0.4",
    "jest": "24.9.0",
    "jest-localstorage-mock": "2.4.0",
    "lint-staged": "9.2.3",
    "msal": "1.2.0-beta.5",
    "prettier": "1.18.2",
    "react": "16.8.0",
    "react-dom": "16.8.0",
    "regenerator-runtime": "0.13.3",
    "rimraf": "3.0.0",
    "terser-webpack-plugin": "2.2.2",
    "ts-loader": "5.3.1",
    "tslint": "5.19.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-microsoft-contrib": "6.2.0",
    "tslint-react": "4.0.0",
    "typescript": "3.2.1",
    "webpack": "4.41.0",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.2.3",
    "webpack-stylish": "0.1.8"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "msal": ">1.1.3"
  },
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">= 5.6.0"
  },
  "keywords": [
    "Azure AD",
    "MSAL",
    "React",
    "OAuth",
    "OAuth2",
    "AAD",
    "Microsoft Authentication Library",
    "Azure Active Directory",
    "Azure"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,json,css,ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint --config tslint.json --fix",
      "git add"
    ]
  }
}
